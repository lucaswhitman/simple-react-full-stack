    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "2.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
